{
  "compilerOptions": {
    "baseUrl": "src",
    "module": "commonjs",
    "target": "es6",
    "paths": {
      "@/*": [ "./*" ],
      "@images/*": [  "./assets/images/*" ],
      "@pages/*": [  "./pages/*" ],
      "@components/*": [  "./components/*" ],
      "@common/*": [  "./components/common/*" ]
    }
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ]
}